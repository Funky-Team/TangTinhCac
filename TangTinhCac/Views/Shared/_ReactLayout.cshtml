
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,700" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/template/open-iconic-bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/template/animate.css">
    <link rel="stylesheet" href="~/Content/template/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/template/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Content/template/magnific-popup.css">
    <link rel="stylesheet" href="~/Content/template/aos.css">
    <link rel="stylesheet" href="~/Content/template/ionicons.min.css">
    <link rel="stylesheet" href="~/Content/template/bootstrap-datepicker.css">
    <link rel="stylesheet" href="~/Content/template/jquery.timepicker.css">
    <link rel="stylesheet" href="~/Content/template/flaticon.css">
    <link rel="stylesheet" href="~/Content/template/icomoon.css">
    <link rel="stylesheet" href="~/Content/template/accordion.css">
    <link rel="stylesheet" href="~/Content/template/floating-button.css">
    <link rel="stylesheet" href="~/Content/template/horizontal-filter.css">
    <link rel="stylesheet" href="~/Content/template/video-gallery.css">
    @*<link rel="stylesheet" href="~/Content/template/_variables.css">
        <link rel="stylesheet" href="~/Content/template/_predefine.css">
        <link rel="stylesheet" href="~/Content/template/breadcrumb.css">*@
    <link rel="stylesheet" href="~/Content/template/style.css">
    <link rel="stylesheet" href="~/Content/Site.css">
    @RenderSection("head", required: false)
</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @*<script src="~/Scripts/template/jquery-migrate-3.0.1.min.js"></script>*@
    @RenderSection("webpack_bundles", required: false)
    <script src="~/Scripts/template/popper.min.js"></script>
    <script src="~/Scripts/template/jquery.easing.1.3.js"></script>
    <script src="~/Scripts/template/jquery.waypoints.min.js"></script>
    <script src="~/Scripts/template/jquery.stellar.min.js"></script>
    <script src="~/Scripts/template/owl.carousel.min.js"></script>
    <script src="~/Scripts/template/jquery.magnific-popup.min.js"></script>
    <script src="~/Scripts/template/aos.js"></script>
    <script src="~/Scripts/template/jquery.animateNumber.min.js"></script>
    <script src="~/Scripts/template/bootstrap-datepicker.js"></script>
    @*<script src="~/Scripts/template/jquery.timepicker.min.js"></script>*@
    <script src="~/Scripts/template/scrollax.min.js"></script>
    @*<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
        <script src="~/Scripts/template/google-map.js"></script>*@
    <script src="~/Scripts/template/main.js"></script>
    <script src="~/Scripts/template/accordion.js"></script>
    <script src="~/Scripts/template/floating-button.js"></script>
    <script src="~/Scripts/template/video-gallery.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
